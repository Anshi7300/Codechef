import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
      Scanner input = new Scanner(System.in);
      int T = input.nextInt();
      while(T >= 1){
         int[] value = new int[3];
         value[0] = input.nextInt();
         value[1] = input.nextInt();
         value[2] = input.nextInt();
         Arrays.sort(value);
         System.out.println(value[1]);
         T--;
        }
	}
}
