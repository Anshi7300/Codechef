import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
      Scanner input = new Scanner(System.in);
      int t = input.nextInt();
      while(t >= 1){
          int a = input.nextInt();
          int b = input.nextInt();
          int c = input.nextInt();
          if(a > b && a > c){
              System.out.println("alice");
            }
          else if(b > a && b > c){
              System.out.println("bob");
            }
          else{
              System.out.println("charlie");
            }
          t--;
        }
	}
}
