import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner input = new Scanner(System.in);
		int N = input.nextInt();
		int a = N*2000;
		System.out.println(a/500);
        
	}
}
