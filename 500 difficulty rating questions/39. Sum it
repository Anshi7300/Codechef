import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner input = new Scanner(System.in);
		int t = input.nextInt();
		while(t >= 1 ){
		    	int x = input.nextInt();
		    	int y = input.nextInt();
		    	int z = input.nextInt();
		    	if((x+y)==z){
		    	    System.out.println("YES");
		    	}
		    	else{
		    	    System.out.println("no");
		    	}
		    	t--;
		}

	}
}
