import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
     Scanner input = new Scanner(System.in);
     int t = input.nextInt();
     while(t >= 1){
         int x = input.nextInt();
         int a = x*4;
         if(a <= 1000){
             System.out.println("yes");
            }
         else{
             System.out.println("no");
            }
            t--;
        }
	}
}
