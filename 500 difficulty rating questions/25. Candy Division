import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner input = new Scanner(System.in);
		int T = input.nextInt();
		while(T >= 1 ){
		    	int N = input.nextInt();
		    	if(N%3 == 0){
		    	    System.out.println("YES");
		    	}
		    	else{
		    	    System.out.println("NO");
		    	}
		    	T--;
		}

	}
}
