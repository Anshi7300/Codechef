import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner input = new Scanner(System.in);
		int T = input.nextInt();
		while(T >= 1){
		    int x = input.nextInt();
		    int y = input.nextInt();
		    int a = (10*x + 90*y);
		    System.out.println(a);
		    T--;
		}

	}
}
