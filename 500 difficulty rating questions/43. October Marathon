import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
      Scanner input = new Scanner(System.in);
      int x = input.nextInt();
      if(x < 3){
          System.out.println("gold");
      }
      else if(x >= 3 && x < 6 ){
          System.out.println("silver");
      }
      else {
          System.out.println("bronze");
      }
	}
}
